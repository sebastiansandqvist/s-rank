{
  "name": "s-rank",
  "version": "1.1.0",
  "description": "Variation on Reddit's Hot Ranking Algorithm",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "jscs index.js --config=./etc/.jscs.json && eslint index.js -c etc/.eslint.json",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -dir etc --report lcovonly -- test/ -R spec",
    "postcoverage": "rm etc/coverage.json",
    "test": "mocha",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastiansandqvist/s-rank.git"
  },
  "keywords": [
    "rank",
    "sort",
    "vote",
    "upvote",
    "hot",
    "algorithm"
  ],
  "author": "sebastian sandqvist",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastiansandqvist/s-rank/issues"
  },
  "homepage": "https://github.com/sebastiansandqvist/s-rank#readme",
  "devDependencies": {
    "@seabass/eslint-config": "^1.0.4",
    "chai": "^3.2.0",
    "codeclimate-test-reporter": "^0.1.0",
    "eslint": "^1.3.1",
    "istanbul": "^0.3.19",
    "jscs": "^2.1.1",
    "mocha": "^2.2.5"
  }
}
